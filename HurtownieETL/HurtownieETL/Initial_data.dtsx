                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @Start DATE = '2020-01-01';
DECLARE @End DATE = '2023-12-31';

DECLARE @AllDates TABLE
(
Date DATETIME,
Rok INT,
MiesiacNO INT,
Miesiac VARCHAR(12),
DzienTygodniaNO INT,
DzienTygodnia VARCHAR(12),
DzienRoboczy VARCHAR(15)
);

WITH Nbrs_3( n ) AS ( SELECT 1 UNION SELECT 0 ),
 
     Nbrs_2( n ) AS ( SELECT 1 FROM Nbrs_3 n1 CROSS JOIN Nbrs_3 n2 ),

     Nbrs_1( n ) AS ( SELECT 1 FROM Nbrs_2 n1 CROSS JOIN Nbrs_2 n2 ),

     Nbrs_0( n ) AS ( SELECT 1 FROM Nbrs_1 n1 CROSS JOIN Nbrs_1 n2 ),

     Nbrs  ( n ) AS ( SELECT 1 FROM Nbrs_0 n1 CROSS JOIN Nbrs_0 n2 )

INSERT INTO @AllDates
SELECT 
DATEADD(DAY, n - 1, @Start) AS Date,
YEAR(DATEADD(DAY, n - 1, @Start)) AS Rok,
MONTH(DATEADD(DAY, n - 1, @Start)) AS MiesiacNO,
CASE MONTH(DATEADD(DAY, n - 1, @Start))
        WHEN 1 THEN 'Styczeń'
        WHEN 2 THEN 'Luty'
        WHEN 3 THEN 'Marzec'
        WHEN 4 THEN 'Kwiecień'
        WHEN 5 THEN 'Maj'
        WHEN 6 THEN 'Czerwiec'
        WHEN 7 THEN 'Lipiec'
        WHEN 8 THEN 'Sierpień'
        WHEN 9 THEN 'Wrzesień'
        WHEN 10 THEN 'Październik'
        WHEN 11 THEN 'Listopad'
        WHEN 12 THEN 'Grudzień'
END AS Miesiac,
DATEPART(WEEKDAY, DATEADD(DAY, n - 1, @Start)) AS DzienTygodniaNO,
CASE DATEPART(WEEKDAY, DATEADD(DAY, n - 1, @Start))
        WHEN 1 THEN 'Niedziela'
        WHEN 2 THEN 'Poniedziałek'
        WHEN 3 THEN 'Wtorek'
        WHEN 4 THEN 'Środa'
        WHEN 5 THEN 'Czwartek'
        WHEN 6 THEN 'Piątek'
        WHEN 7 THEN 'Sobota'
 END AS DzienTygodnia,
CASE DATEPART(WEEKDAY, DATEADD(DAY, n - 1, @Start))
        WHEN 1 THEN 'Dzień wolny'
        WHEN 7 THEN 'Dzień wolny'
        ELSE 'Dzień roboczy'
END AS DzienRoboczy
FROM 
    ( SELECT ROW_NUMBER() OVER (ORDER BY n) AS n FROM Nbrs ) D
WHERE 
    n &lt;= DATEDIFF(DAY, @Start, @End) + 1;

SELECT * FROM @AllDates;</property>
